public class Employee{
    int empId;
    String empName;
    Employee(int id,String name)
    {
        empId=id;
        empName=name;
    }
    void info(){
        System.out.println("Id: "+empId+" Name: "+empName);
    }
    public static void main(String args[]){
        Employee obj1=new Employee(1002,"Raihan");
        Employee obj2=new Employee(1188,"Sabrina");
        obj1.info();
        obj2.info();
    }
}
